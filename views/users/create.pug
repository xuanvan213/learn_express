h1 Create user 
form(action="/users/create", method="POST")
    label(for="name") User Name
        input(type="text", name="name", id="name", value=`${(user !== undefined && user.name !== undefined ? user.name : '')}`)
    br
    label(for="age") Age
        input(type="text", name="age", id="age", value=`${(user !== undefined && user.age !== undefined ? user.age : '')}`)
    br
    button Submit